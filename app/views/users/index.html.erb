<h1> Users </h1>
    <table>
        <thead>
            <tr>
                <th>Nome</th>
                <th>Email</th>
                <th></th>
            </tr>
        </thead>
        <tbody>
            <% @users.each do |user| %>
                <tr>
                    <td><%= user.nome %></td>
                    <td><%= user.email %></td>
                    <td><%= link_to "Show", user %></td>
                </tr>
            <%end%>
        </tbody>
    </table>